config_version=5

[application]

config/name="Savory Secrets"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

PartyController="*res://Party/party_controller.tscn"
PlayerInventoryController="*res://Inventory/player_inventory_controller.tscn"
Actions="*res://Cooking/actions.gd"
LocationEvents="*res://Locations/location_events.gd"
UiEvents="*res://Global/ui_events.gd"
SaveLoad="*res://Saving/save_load.tscn"
Settings="*res://Menus/settings.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
RecipeBookController="*res://Recipes/Books/recipe_book_controller.tscn"
StationController="*res://Cooking/Stations/station_controller.tscn"

[debug]

gdscript/warnings/integer_division=0

[dialogic]

directories/dch_directory={
"chef": "res://Timelines/chef.dch"
}
directories/dtl_directory={
"campfire_tutorial": "res://Timelines/Tutorials/campfire_tutorial.dtl",
